type QueryActivityReport {
  ActivityReport(_id: String!): ActivityReport!
  ActivityReports: [ActivityReport!]!
}

type Mutation {
  createActivityReport(input: NewActivityReport): ActivityReport!
}

input NewActivityReport {
  Timestamp: String!
  Qty: Int!
  AvgGain: Float!
}

type ActivityReport {
  _id: String!
  Timestamp: String!
  Qty: Int!
  AvgGain: Float!
}
